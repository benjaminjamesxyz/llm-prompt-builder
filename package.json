{
  "name": "llm-prompt-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.3.5",
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "build:wasm": "cd wasm && wasm-pack build --target web --out-dir ../src/wasm-pkg",
    "typecheck": "tsc --noEmit",
    "preview": "rsbuild preview",
    "test": "vitest",
    "deploy": "bun run build && wrangler pages deploy dist"
  },
  "dependencies": {
    "dompurify": "^3.3.1",
    "js-yaml": "^4.1.1",
    "preact": "^10.28.1",
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.7.1",
    "@rsbuild/plugin-preact": "^1.7.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "@types/prismjs": "^1.26.5",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@tailwindcss/postcss": "^4.1.18",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "jsdom": "^27.4.0"
  }
}
